{
  "expo": {
    "name": "PontoGorilla",
    "slug": "PontoGorilla",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/DlLogo1024.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/transparent.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/DlLogo1080.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "package": "com.davidlucask.PontoGorilla"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "7f84aaad-6a3b-484e-8582-c275746d2810"
      }
    }
  }
}
